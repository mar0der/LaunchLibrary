# My other pods

def pods
    pod 'Alamofire', '~> 4.7'
    pod 'SVProgressHUD', '~> 2.2'
    pod 'moa', '~> 9.0'
end

target 'LaunchLibraryIOS' do
    platform :ios, '9.0'
    use_frameworks!
    pods
end

target 'LaunchLibraryTVOS' do
    platform :tvos, '9.0'
    use_frameworks!
    pods
end

target 'Launch Library Extension' do
    platform :watchos , '4.0'
    use_frameworks!
    pod 'Alamofire', '~> 4.7'
end

